<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width">
    <title>{% block metatagsTitle %}{{ metatags.title|default('') }}{% endblock %}</title>
    <meta name="keywords" content="{% block metatagsKeywords %}{{ metatags.keywords }}{% endblock %}">
    <meta name="description" content="{% block metatagsDescription %}{{ metatags.description }}{% endblock %}">
    {% block stylesheets %}
        <link href="/css/main.min.css" rel="stylesheet">
    {% endblock %}
</head>
<body>
{% block body %}
    <div class="wrapper">
        {% block mainHeader %}
            {# include 'blocks/header/header-block.twig' #}
        {% endblock %}

        {% block mainBody %}
            <div class="main">
                {% block main %}{% endblock %}
            </div>
        {% endblock %}
    </div>
    {% block mainFooter %}
        {# include 'blocks/footer/footer-block.twig' #}
    {% endblock %}
{% endblock %}

{% block javascripts %}
    <script src="//ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="js/fallback/jquery-3.1.1.min.js"><\/script>')</script>
    <script src="/js/main.min.js"></script>
{% endblock %}

<script type="text/template" data-el="bootstrap-select.template.caret">
   {{- appMacro.svgIcon(
        'caret', 
        'images/svg-symbols.svg',
        'icon icon--svg icon--caret'
    ) -}}
</script>
</body>
</html>